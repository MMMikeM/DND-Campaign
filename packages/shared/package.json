{
  "name": "@tome-master/shared",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "type": "module",
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "scripts": {
    "build": "bunchee",
    "check": "tsc --noEmit",
    "db:generate": "NODE_OPTIONS='--import tsx' drizzle-kit generate",
    "db:migrate": "NODE_OPTIONS='--import tsx' drizzle-kit migrate",
    "db:studio": "NODE_OPTIONS='--import tsx' drizzle-kit studio",
    "dev": "bunchee --watch",
    "lint:fix": "biome check . --write",
    "lint": "biome check .",
    "proxy": "tsx src/proxy.ts",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@swc/helpers": "^0.5.17",
    "drizzle-orm": "catalog:",
    "pg": "^8.16.0",
    "pgvector": "^0.2.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/node": "catalog:",
    "@types/pg": "^8.15.2",
    "bunchee": "^6.5.2",
    "drizzle-kit": "^0.31.1",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@9.14.2"
}
